
{

float vet[3],soma=0,media; int posicao;

for (posicao=0;posicao<3;posicao++)

{

scanf ("%f",&vet[posicao]);

soma=soma+vet[posicao];

}

media=soma/3;

printf ("numeros maiores que media %.2f \n",media);

for (posicao=0;posicao<3;posicao++)

{

if (vet[posicao] >= media)

printf ("%.2f \n",vet[posicao]);

}

}
